{
  "name": "avtoservis",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:fast": "expo start --offline --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "clear-cache": "expo start -c",
    "clear-all": "expo start --clear && npm run clear-metro && npm run clear-babel",
    "clear-metro": "rm -rf node_modules/.cache/metro",
    "clear-babel": "rm -rf node_modules/.cache/babel-loader",
    "android-dev": "expo start --android --dev-client",
    "ios-dev": "expo start --ios --dev-client",
    "prebuild": "expo prebuild",
    "optimize": "npm run clear-all && npm run start:fast",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:tokens": "node tests/runTokenTests.js",
    "test:security": "jest tests/authSecurityTest.js --verbose"
  },
  "dependencies": {
    "@expo/metro-config": "^0.20.14",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "^8.4.1",
    "@react-native-ml-kit/text-recognition": "^2.0.0",
    "@react-native-picker/picker": "^2.11.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.14",
    "@react-navigation/stack": "^6.3.25",
    "axios": "^1.6.7",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "expo": "~53.0.24",
    "expo-auth-session": "~5.5.2",
    "expo-camera": "~16.1.11",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-document-picker": "~13.1.6",
    "expo-file-system": "~18.1.11",
    "expo-image-manipulator": "~13.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-localization": "~16.1.6",
    "expo-media-library": "~17.1.7",
    "expo-notifications": "~0.31.4",
    "expo-print": "~14.0.0",
    "expo-secure-store": "~14.2.4",
    "expo-sharing": "^13.1.5",
    "expo-status-bar": "~2.2.3",
    "expo-web-browser": "~14.1.3",
    "i18next": "^23.10.0",
    "jimp": "^1.6.0",
    "jwt-decode": "^4.0.0",
    "metro": "^0.82.0",
    "metro-config": "^0.82.0",
    "metro-minify-terser": "^0.82.2",
    "metro-resolver": "^0.82.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "^14.1.0",
    "react-native": "0.79.6",
    "react-native-calendars": "^1.1312.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.11.2",
    "react-native-swipe-list-view": "^3.2.9",
    "react-native-toast-notifications": "^3.4.0",
