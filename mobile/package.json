    "babel-plugin-module-resolver": "^5.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "react-test-renderer": "19.0.0",
    "typescript": "^5.3.0"
  },
  "private": true
